{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "interface-name": [false],
    "member-access": [false],
    "ordered-imports": [false],
    "no-empty-interface": false,
    "object-literal-sort-keys": false,
    "jsx-no-lambda": false,
    "no-any": false,
    "interface-over-type-literal": false
  }
}
